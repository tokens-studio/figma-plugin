        "npm-run-all": "^4.1.5",
        "postcss-cli": "^8.3.1",
        "prettier": "^2.0.5",
        "serve": "^11.3.2",
        "style-loader": "^0.23.1",
        "svg-url-loader": "^7.1.1",
