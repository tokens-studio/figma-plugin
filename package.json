        "url-loader": "^2.1.0",
        "webpack": "^4.41.4",
        "webpack-cli": "^3.3.6"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{ts,tsx}": [
            "./eslint-non-blocking.sh",
            "git add"
        ],
        "src/**/*.{js,jsx,css,json}": [
            "prettier --write",
            "git add"
        ]
    }
