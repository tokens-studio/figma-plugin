        "axios": "^0.21.2",
        "classnames": "^2.3.1",
        "color2k": "^1.2.4",
        "core-js": "^3.9.1",
        "cypress-react-selector": "^2.3.6",
        "dnd-core": "^12.0.1",
        "dot-object": "^2.1.3",
